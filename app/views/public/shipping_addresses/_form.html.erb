<%= form_with model:@member, local:true do |f| %>
        
        <div class="form-group">
            <%= f.label :"郵便番号" %>
            <%= f.text_field :deliver_code, class: "form-control name" %>
        </div>

        <div class="form-group">
            <%= f.label :"住所" %>
            <%= f.text_field :deliver_address, class: "form-control name" %>
        </div>

        <div class="form-group">
            <%= f.label :"宛名" %>
            <%= f.text_field :address, class: "form-control name" %>
        </div>

        <div class="form-group">
            <%= f.label :"メールアドレス" %>
            <%= f.text_field :email, class: "form-control name" %>
        </div>

        <div class="form-group">
           <%= f.submit "変更内容を保存", class: 'btn btn-success' %>
        </div>

        <div><%=link_to "退会する", unsubscribe_members_path, class: "btn btn-danger" %></div>
     <% end %>